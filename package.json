{
  "publisher": "aniongithub",
  "name": "devcontainer",
  "repository": "https://github.com/aniongithub/devcontainer",
  "displayName": "devcontainer",
  "description": "devcontainer is a VS Code extension to create and manage one or more remote containers for development",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:devcontainer"
  ],
  "main": "./out/devcontainer.js",
  "files": [
    "src/scripts/**"
  ],
  "contributes": {
    "commands": [
      {
        "command": "devcontainer",
        "title": "devcontainer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "extensionDependencies": [
    "ms-vscode-remote.remote-containers"
  ]
}
